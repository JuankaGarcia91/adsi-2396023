doctype
html(lang="en")
    head
        meta(charset="UTF-8")        
        title Challenge
        style.
            ul {
                list-style:none;
                padding: 0;
            }
            
    body 
        h1 challenge 
        ul
            - var color = 250
            - for(var i = 1; i <= 10; i++)
                li(style={
                    'background-color': 'rgb('+color+','+color+',250)',
                    border: '1px solid #000',
                    padding: '30px',
                    margin: '4px'
                })
                - color-=25 
